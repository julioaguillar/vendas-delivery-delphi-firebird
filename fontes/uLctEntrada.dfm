  object qryLancamentoEntrada: TIBOQuery
    DeleteSQL.Strings = (
      'DELETE FROM LCT_ENTRADA LCT_ENTRADA'
      'WHERE'
      '   ID_ENTRADA = :OLD_ID_ENTRADA')
    EditSQL.Strings = (
      'UPDATE LCT_ENTRADA LCT_ENTRADA SET'
      '   LCT_ENTRADA.ID_ENTRADA = :ID_ENTRADA, /*PK*/'
      '   LCT_ENTRADA.DT_ENTRADA = :DT_ENTRADA,'
      '   LCT_ENTRADA.HR_ENTRADA = :HR_ENTRADA,'
      '   LCT_ENTRADA.NR_ENTRADA = :NR_ENTRADA,'
      '   LCT_ENTRADA.DS_ENTRADA = :DS_ENTRADA,'
      '   LCT_ENTRADA.NR_QUANTIDADEPRODUTOS = :NR_QUANTIDADEPRODUTOS,'
      '   LCT_ENTRADA.VR_TOTALENTRADA = :VR_TOTALENTRADA,'
      '   LCT_ENTRADA.CD_FORNECEDOR = :CD_FORNECEDOR'
      'WHERE'
      '   ID_ENTRADA = :OLD_ID_ENTRADA')
    FieldsDisplayFormat.Strings = (
      'LCT_ENTRADA.VR_TOTALENTRADA=#,###,###,##0.00')
    IB_Connection = dmPrincipal.IB_Connection
    IB_Transaction = trnsCadPadrao
    InsertSQL.Strings = (
      'INSERT INTO LCT_ENTRADA('
      '   ID_ENTRADA, /*PK*/'
      '   DT_ENTRADA,'
      '   HR_ENTRADA,'
      '   NR_ENTRADA,'
      '   DS_ENTRADA,'
      '   NR_QUANTIDADEPRODUTOS,'
      '   VR_TOTALENTRADA,'
      '   CD_FORNECEDOR)'
      'VALUES ('
      '   :ID_ENTRADA,'
      '   :DT_ENTRADA,'
      '   :HR_ENTRADA,'
      '   :NR_ENTRADA,'
      '   :DS_ENTRADA,'
      '   :NR_QUANTIDADEPRODUTOS,'
      '   :VR_TOTALENTRADA,'
      '   :CD_FORNECEDOR)')
    PreparedEdits = True
    PreparedInserts = True
    RecordCountAccurate = True
    SQL.Strings = (
      'SELECT LCT_ENTRADA.ID_ENTRADA,'
      '       LCT_ENTRADA.DT_ENTRADA,'
      '       LCT_ENTRADA.HR_ENTRADA,'
      '       LCT_ENTRADA.NR_ENTRADA,'
      '       LCT_ENTRADA.DS_ENTRADA,'
      '       LCT_ENTRADA.NR_QUANTIDADEPRODUTOS,'
      '       LCT_ENTRADA.VR_TOTALENTRADA,'
      '       LCT_ENTRADA.CD_FORNECEDOR,'
      '       CAD_FORNECEDOR.DS_NOME'
      'FROM LCT_ENTRADA'
      
        'LEFT JOIN CAD_FORNECEDOR ON(CAD_FORNECEDOR.CD_FORNECEDOR = LCT_E' +
        'NTRADA.CD_FORNECEDOR)'
      'WHERE LCT_ENTRADA.ID_ENTRADA IS NOT NULL'
      '      /*AND LCT_ENTRADA.CD_FORNECEDOR = :CD_FORNECEDOR*/'
      
        '      /*AND LCT_ENTRADA.DT_ENTRADA BETWEEN :DT_REFERENCIAINICIO ' +
        'AND :DT_REFERENCIAFIM*/'
      
        'ORDER BY LCT_ENTRADA.DT_ENTRADA DESC, LCT_ENTRADA.HR_ENTRADA DES' +
        'C')
    Left = 159
    Top = 269
  end
