  object qryConsVendas: TIBOQuery
    Params = <
      item
        DataType = ftUnknown
        Name = 'DT_REFERENCIAINICIO'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'DT_REFERENCIAFIM'
        ParamType = ptInput
      end>
    IB_Connection = dmPrincipal.IB_Connection
    PreparedEdits = True
    PreparedInserts = True
    RecordCountAccurate = True
    SQL.Strings = (
      
        'SELECT SUM( COALESCE(LCT_VENDA.VR_TOTALVENDA, 0.0)) AS VR_TOTALV' +
        'ENDA'
      'FROM LCT_VENDA'
      
        'WHERE LCT_VENDA.DT_VENDA BETWEEN :DT_REFERENCIAINICIO AND :DT_RE' +
        'FERENCIAFIM'
      ''
      'UNION ALL'
      ''
      
        'SELECT SUM( COALESCE(LCT_CONTASRECEBER.VR_CONTARECEBER, 0.0)) AS' +
        ' VR_TOTALVENDA'
      'FROM LCT_CONTASRECEBER'
      
        'WHERE LCT_CONTASRECEBER.DT_RECEBIMENTO BETWEEN :DT_REFERENCIAINI' +
        'CIO AND :DT_REFERENCIAFIM')
    Left = 732
    Top = 8
  end
  object qryConsEntradas: TIBOQuery
    Params = <
      item
        DataType = ftUnknown
        Name = 'DT_REFERENCIAINICIO'
        ParamType = ptInput
      end
      item
        DataType = ftUnknown
        Name = 'DT_REFERENCIAFIM'
        ParamType = ptInput
      end>
    IB_Connection = dmPrincipal.IB_Connection
    PreparedEdits = True
    PreparedInserts = True
    RecordCountAccurate = True
    SQL.Strings = (
      
        'SELECT SUM( COALESCE(LCT_ENTRADA.VR_TOTALENTRADA, 0.0) ) AS VR_T' +
        'OTALENTRADA'
      'FROM LCT_ENTRADA'
      
        'WHERE LCT_ENTRADA.DT_ENTRADA BETWEEN :DT_REFERENCIAINICIO AND :D' +
        'T_REFERENCIAFIM'
      ''
      'UNION ALL'
      ''
      
        'SELECT SUM( COALESCE(LCT_CONTASPAGAR.VR_CONTAPAGAR, 0.0)) AS VR_' +
        'TOTALENTRADA'
      'FROM LCT_CONTASPAGAR'
      
        'WHERE LCT_CONTASPAGAR.DT_VENCIMENTO BETWEEN :DT_REFERENCIAINICIO' +
        ' AND :DT_REFERENCIAFIM')
    Left = 764
    Top = 8
  end
