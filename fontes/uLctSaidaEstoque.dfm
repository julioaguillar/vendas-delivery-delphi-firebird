    EditSQL.Strings = (
      'UPDATE LCT_SAIDAPRODUTO LCT_SAIDAPRODUTO SET'
      '   LCT_SAIDAPRODUTO.ID_SAIDAPRODUTO = :ID_SAIDAPRODUTO, /*PK*/'
      '   LCT_SAIDAPRODUTO.ID_PRODUTO = :ID_PRODUTO,'
