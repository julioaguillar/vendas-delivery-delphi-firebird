      'WHERE'
      '   ID_SAIDAPRODUTO = :OLD_ID_SAIDAPRODUTO')
    FieldsDisplayFormat.Strings = (
      'LCT_ENTRADAPRODUTO.VR_PRECOPRODUTO=#,###,##0.00'
      'LCT_ENTRADAPRODUTO.VR_TOTAL=#,###,##0.00')
    FieldsEditMask.Strings = (
      'LCT_ENTRADAPRODUTO.DT_ENTRADAPRODUTO=!99/99/9999;1;_')
    IB_Connection = dmPrincipal.IB_Connection
    IB_Transaction = trnsCadPadrao
    InsertSQL.Strings = (
      'INSERT INTO LCT_SAIDAPRODUTO('
      '   ID_SAIDAPRODUTO, /*PK*/'
      '   ID_PRODUTO,'
      '   DT_SAIDAPRODUTO,'
      '   QT_SAIDAPRODUTO,'
      '   VR_PRECOPRODUTO,'
      '   VR_TOTAL,'
      '   DS_OBSERVACAO)'
      'VALUES ('
      '   :ID_SAIDAPRODUTO,'
      '   :ID_PRODUTO,'
      '   :DT_SAIDAPRODUTO,'
      '   :QT_SAIDAPRODUTO,'
      '   :VR_PRECOPRODUTO,'
      '   :VR_TOTAL,'
      '   :DS_OBSERVACAO)')
    RecordCountAccurate = True
    AfterOpen = qryLctSaidaProdutoAfterOpen
    SQL.Strings = (
      'SELECT LCT_SAIDAPRODUTO.ID_SAIDAPRODUTO,'
      '       LCT_SAIDAPRODUTO.ID_PRODUTO,'
      '       LCT_SAIDAPRODUTO.DT_SAIDAPRODUTO,'
      '       LCT_SAIDAPRODUTO.QT_SAIDAPRODUTO,'
      '       LCT_SAIDAPRODUTO.VR_PRECOPRODUTO,'
      '       LCT_SAIDAPRODUTO.VR_TOTAL,'
      '       LCT_SAIDAPRODUTO.DS_OBSERVACAO,'
      '       CAD_PRODUTO.DS_NOME'
      'FROM LCT_SAIDAPRODUTO'
      
        'LEFT JOIN CAD_PRODUTO ON(CAD_PRODUTO.ID_PRODUTO = LCT_SAIDAPRODU' +
        'TO.ID_PRODUTO)'
      
        'WHERE ( ( :FG_TODOSTRODUTOS = '#39'SIM'#39' ) OR ( LCT_SAIDAPRODUTO.ID_P' +
        'RODUTO = :ID_PRODUTO ) ) AND'
      
        '      ( ( :FG_TODOSPERIODOS = '#39'SIM'#39' ) OR ( LCT_SAIDAPRODUTO.DT_S' +
        'AIDAPRODUTO BETWEEN :DT_INICIO AND :DT_FIM ) )'
      'ORDER BY LCT_SAIDAPRODUTO.DT_SAIDAPRODUTO DESC')
    FieldOptions = []
    Left = 64
    Top = 320
