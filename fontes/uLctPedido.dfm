  object qryLancamentoPedido: TIBOQuery
    DeleteSQL.Strings = (
      'DELETE FROM LCT_PEDIDO LCT_PEDIDO'
      'WHERE'
      '   ID_PEDIDO = :OLD_ID_PEDIDO')
    EditSQL.Strings = (
      'UPDATE LCT_PEDIDO LCT_PEDIDO SET'
      '   LCT_PEDIDO.ID_PEDIDO = :ID_PEDIDO, /*PK*/'
      '   LCT_PEDIDO.DT_PEDIDO = :DT_PEDIDO,'
      '   LCT_PEDIDO.HR_PEDIDO = :HR_PEDIDO,'
      '   LCT_PEDIDO.NR_PEDIDO = :NR_PEDIDO,'
      '   LCT_PEDIDO.DS_OBSERVACAO = :DS_OBSERVACAO,'
      '   LCT_PEDIDO.NR_QUANTIDADEPRODUTOS = :NR_QUANTIDADEPRODUTOS,'
      '   LCT_PEDIDO.VR_TOTALPEDIDO = :VR_TOTALPEDIDO,'
      '   LCT_PEDIDO.CD_CLIENTE = :CD_CLIENTE'
      'WHERE'
      '   ID_PEDIDO = :OLD_ID_PEDIDO')
    FieldsDisplayFormat.Strings = (
      'LCT_PEDIDO.DT_PEDIDO=dd/MM/yyyy'
      'LCT_PEDIDO.HR_PEDIDO=hh:mm'
      'LCT_PEDIDO.VR_TOTALPEDIDO=,0.00')
    IB_Connection = dmPrincipal.IB_Connection
    IB_Transaction = trnsCadPadrao
    InsertSQL.Strings = (
      'INSERT INTO LCT_PEDIDO('
      '   ID_PEDIDO, /*PK*/'
      '   DT_PEDIDO,'
      '   HR_PEDIDO,'
      '   NR_PEDIDO,'
      '   DS_OBSERVACAO,'
      '   NR_QUANTIDADEPRODUTOS,'
      '   VR_TOTALPEDIDO,'
      '   CD_CLIENTE)'
      'VALUES ('
      '   :ID_PEDIDO,'
      '   :DT_PEDIDO,'
      '   :HR_PEDIDO,'
